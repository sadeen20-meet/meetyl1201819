from turtle import Turtle
import turtle
turtle.begin_poly()
turtle.register_shape("strawberry",((50,0),(80,-50),(50,-100),(0,100),(-30,-50),(0,0)))
turtle.mainloop()
class Hexagon(Turtle):
	def __init__(self,size):
		Turtle.__init__(self)
	self.shapesize(size)
	self.shape((50,0),(80,-50),(50,-100),(0,100),(-30,-50),(0,0))




